# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page
GET     /                           controllers.SearchController.index()

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
GET   /search/searchVideos   controllers.SearchController.searchVideos(query: String)

# Route for Individual Parts
GET     /channel/:channelId         controllers.SearchController.showChannelProfile(channelId: String)
GET     /search/MoreStats          controllers.SearchController.MoreStats(query: String)
